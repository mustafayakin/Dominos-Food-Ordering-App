--
-- TOC entry 255 (class 1255 OID 16667)
-- Name: indirimsizfiyat(double precision, integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.indirimsizfiyat(tutar double precision, discount integer) RETURNS double precision
    LANGUAGE plpgsql
    AS $$
begin
tutar = (100*tutar) / (100-discount);
return tutar;
end;
$$;


ALTER FUNCTION public.indirimsizfiyat(tutar double precision, discount integer) OWNER TO postgres;

