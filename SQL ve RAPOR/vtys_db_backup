CREATE FUNCTION public.deleteuye() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
  DELETE FROM seviyeli_uyeler
  WHERE seviyeli_uyeler.kullanici_id = OLD.id;
  RETURN NEW;
END
$$;


ALTER FUNCTION public.deleteuye() OWNER TO postgres;

