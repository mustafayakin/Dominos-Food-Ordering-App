--
-- TOC entry 266 (class 1255 OID 16675)
-- Name: yemekfiyatdegisikligi(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.yemekfiyatdegisikligi() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    IF NEW."tutar" <> OLD."tutar" THEN
        INSERT INTO "yemekfiyatdegisikligi"("urunno", "eskifiyat", "yenifiyat", "degisiklik_tarih")
        VALUES(OLD."id", OLD."tutar", NEW."tutar", CURRENT_TIMESTAMP::TIMESTAMP);
    END IF;

    RETURN NEW;
END;
$$;


ALTER FUNCTION public.yemekfiyatdegisikligi() OWNER TO postgres;

--
